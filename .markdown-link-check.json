{
  "aliveStatusCodes": [
    200,
    206,
    999,
    400
  ],
  "fallbackRetryDelay": "30s",
  "ignorePatterns": [
    {
      "pattern": "^#"
    },
    {
      "pattern": "^Taskfile.yml"
    },
    {
      "pattern": "^.github/workflows/"
    },
    {
      "pattern": "^docs/templates/"
    },
    {
      "pattern": "CHANGELOG.md"
    },
    {
      "pattern": "compare"
    }
  ],
  "retryCount": 5,
  "retryOn429": true,
  "timeout": "20s"
}
